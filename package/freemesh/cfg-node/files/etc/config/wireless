config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi'
	option channel '1'
	option band '2g'
	option htmode 'HE20'
	option cell_density '0'
	option disabled '0'
	option country 'US'
	option channel '10'
	option txpower '23'
	option noscan '1'
	option short_preamble '1'

config wifi-iface 'ap_two'
	option device 'radio0'
	option network 'default'
	option mode 'ap'
	option ifname 'phy0-ap0'
	option bssid ''
	option hidden '0'
	option disassoc_low_ack '0'
	option ssid 'FreeMesh2GHz'
	option encryption 'psk2'
	option key '9876543210'
	option nasid ''
	option mobility_domain ''
	option ft_over_ds '0'
	option ieee80211r '1'
    option ft_psk_generate_local '1' 
    option dtim_period '3'           
    option ieee80211k '1'    
    option wnm_sleep_mode '1'        
    option wnm_sleep_mode_no_keys '1'
    option bss_transition '1'


config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi+1'
	option channel '36'
	option band '5g'
	option htmode 'HE80'
	option cell_density '0'
	option noscan '1'
	option short_preamble '1'
	option disabled '0'
	option country 'US'
	option txpower '25'
	option channel '40'

config wifi-iface 'ap_five'
	option device 'radio1'
	option network 'default'
	option mode 'ap'
	option ifname 'phy1-ap0'
	option bssid ''
	option hidden '0'
	option encryption 'psk2'
	option ssid 'FreeMesh5GHz'
	option key '9876543210'
	option ft_over_ds '0'
	option ieee80211r '1'
    option ft_psk_generate_local '1' 
    option dtim_period '3'           
    option ieee80211k '1'    
    option wnm_sleep_mode '1'        
    option wnm_sleep_mode_no_keys '1'
    option bss_transition '1'
	option mobility_domain ''
	option nasid ''

config wifi-iface 'mesh_five'
	option ifname 'mesh1'
	option device 'radio1'
	option hidden '0'
	option encryption 'sae'
	option network 'mesh'
	option mode 'mesh'
	option mesh_ttl '1'
	option mesh_fwding '0'
	option mesh_id ''
	option key ''
	option mcast_rate '24000'
